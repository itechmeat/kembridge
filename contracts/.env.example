PRIVATE_KEY=_wallet_private_key_

BRIDGE_CONTRACT_ADDRESS=YOUR_DEPLOYED_CONTRACT_ADDRESS
BRIDGE_CONTRACT_DEPLOYED_BLOCK=YOUR_DEPLOYMENT_BLOCK_NUMBER
