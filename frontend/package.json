{
  "name": "kembridge-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host 0.0.0.0",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "docker:dev": "docker-compose up frontend",
    "docker:build": "docker-compose build frontend"
  },
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "overrides": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0",
      "near-api-js": "^6.2.2",
      "@near-js/accounts": "^2.0.1",
      "@near-js/crypto": "^2.0.1",
      "@near-js/keystores": "^2.0.1",
      "@near-js/keystores-browser": "^2.0.1",
      "@near-js/keystores-node": "^2.0.1",
      "@near-js/providers": "^2.0.1",
      "@near-js/signers": "^2.0.1",
      "@near-js/transactions": "^2.0.1",
      "@near-js/types": "^2.0.1",
      "@near-js/utils": "^2.0.1"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "near-api-js"
      ],
      "allowedVersions": {
        "near-api-js": "6",
        "react": "19",
        "react-dom": "19"
      }
    }
  },
  "dependencies": {
    "@near-wallet-selector/core": "^9.0.3",
    "@near-wallet-selector/modal-ui": "^9.0.3",
    "@near-wallet-selector/my-near-wallet": "^9.0.3",
    "@near-wallet-selector/nightly": "^9.1.0",
    "@near-wallet-selector/react-hook": "^9.0.3",
    "@rainbow-me/rainbowkit": "^2.2.8",
    "@tanstack/react-query": "^5.83.0",
    "@tanstack/react-query-devtools": "^5.83.0",
    "@walletconnect/ethereum-provider": "^2.21.5",
    "axios": "^1.10.0",
    "near-api-js": "^6.2.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.3",
    "viem": "^2.31.7",
    "wagmi": "^2.15.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.31.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "sass": "^1.89.2",
    "typescript": "^5.8.3",
    "vite": "^7.0.4"
  }
}
